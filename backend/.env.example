# GeoBot Backend Environment Variables
# Copy this file to .env and fill with your actual values

# Application
DEBUG=False
HOST=127.0.0.1
PORT=8000
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:5173"]

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_BUCKET=pdfs
DATABASE_URL=postgresql://user:password@host:port/database

# AI Configuration (loaded from user config, not environment)
# These are managed through the UI configuration screen
# AI_PROVIDER=groq
# AI_MODEL=llama-3.3-70b-versatile

# RAG Configuration
EMBEDDING_MODEL=intfloat/e5-large-v2
EMBEDDING_DIMENSION=1024
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
TOP_K_RESULTS=5

# Storage Paths
DATA_DIR=C:\Users\YourUser\GeoBot\data
PROJECTS_DIR=C:\Users\YourUser\GeoBot\projects
CACHE_DIR=C:\Users\YourUser\GeoBot\cache

# Processing
MAX_WORKERS=4
ASYNC_PROCESSING=True

# Logging
LOG_LEVEL=INFO
